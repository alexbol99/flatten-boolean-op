{
  "name": "@flatten-js/boolean-op",
  "version": "1.0.16",
  "description": "Boolean operations on polygons",
  "main": "dist/main.cjs.js",
  "umd:main": "dist/main.umd.js",
  "unpkg": "dist/main.umd.js",
  "module": "dist/main.esm.js",
  "scripts": {
    "test": "mocha --require @babel/register --reporter spec",
    "generate-docs": "jsdoc -c ./.jsdoc.json --verbose",
    "npm-publish-norun": "npm version patch; git push && git push --tags; npm publish",
    "npm-publish": "npm publish --access public",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- --require @babel/register -R spec test/*",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexbol99/flatten-boolean-op.git"
  },
  "keywords": [
    "flatten-js",
    "geometry",
    "polygon",
    "boolean operations",
    "union",
    "intersect",
    "clipping",
    "subtract"
  ],
  "author": "Alex Bol <alexbol99@gmail.com> (https://github.com/alexbol99)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexbol99/flatten-boolean-op/issues"
  },
  "homepage": "https://github.com/alexbol99/flatten-boolean-op#readme",
  "engines": {
    "node": ">=4.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "jsdom": "9.12.0",
    "jsdom-global": "2.1.1",
    "minami": "^1.2.3",
    "mocha": "^5.2.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "dependencies": {
    "@flatten-js/core": "^1.0.11",
    "@flatten-js/utils": "^1.0.0"
  }
}

